[user]
  name = yuzuemon
  email = killerqueen64@gmail.com
[core]
  excludesfile = ~/dotfiles/.gitignore_global
  precomposeunicode = true
[color]
  diff = auto
  ui = true
[alias]
  a  = add
  au = add -u
  cm = commit -m
  co = checkout
  cb = checkout -b
  cp = cherry-pick
  d  = diff
  dm = diff master
  dw = diff --color-words
  dc = diff --cached
  ds = diff --staged
  d1 = diff HEAD~
  d2 = diff HEAD~~
  d3 = diff HEAD~~~
  f  = fetch
  m  = merge
  p  = pull
  ps = push
  psu= push -u
  s  = status
  edit-unmerged = "!f() { git ls-files --unmerged | cut -f2 | sort -u ; }; vim `f`"
  tree = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
[push]
  default = simple
[pager]
  diff = /usr/local/opt/git/share/git-core/contrib/diff-highlight/diff-highlight | less
  log  = /usr/local/opt/git/share/git-core/contrib/diff-highlight/diff-highlight | less
  show = /usr/local/opt/git/share/git-core/contrib/diff-highlight/diff-highlight | less
[credential]
  helper = osxkeychain
